            word-break: normal;
            word-spacing: normal;
        }

        .richText  pre[class*="language-"]::-moz-selection, pre[class*="language-"] ::-moz-selection,
        .richText code[class*="language-"]::-moz-selection, code[class*="language-"] ::-moz-selection {
            background: #b3d4fc;
            text-shadow: none;
        }

        .richText pre[class*="language-"]::selection, pre[class*="language-"] ::selection,
        .richText  code[class*="language-"]::selection, code[class*="language-"] ::selection {
            background: #b3d4fc;
            text-shadow: none;
        }

        @media print {
            code[class*="language-"],
            pre[class*="language-"] {
                text-shadow: none;
            }
        }

        /* Code blocks */
        .richText pre[class*="language-"] {
            margin: .5em 0;
            overflow: auto;
            padding: 1em;
        }

        .richText :not(pre) > code[class*="language-"],
        .richText pre[class*="language-"] {
            background: #fff;
        }

        /* Inline code */
        .richText :not(pre) > code[class*="language-"] {
            border-radius: .3em;
            padding: .1em;
        }

        .richText  .token.comment,
        .richText   .token.prolog,
        .richText  .token.doctype,
        .richText  .token.cdata {
            color: slategray;
        }

        .richText   .token.punctuation {
            color: #999;
        }

        .richText   .namespace {
            opacity: .7;
        }

        .richText  .token.property,
        .richText   .token.tag,
        .richText   .token.boolean,
        .richText    .token.number,
        .richText    .token.constant,
        .richText    .token.symbol,
        .richText    .token.deleted {
            color: #905;
        }

        .richText    .token.selector,
        .richText    .token.attr-name,
        .richText    .token.string,
        .richText     .token.char,
        .richText    .token.builtin,
        .richText    .token.inserted {
            color: #690;
        }

        .richText    .token.operator,
        .richText    .token.entity,
        .richText    .token.url,
        .richText    .language-css .token.string,
        .richText    .style .token.string {
            background: hsla(0, 0%, 100%, .5);
            color: #a67f59;
        }

        .richText   .token.atrule,
        .richText   .token.attr-value,
        .richText    .token.keyword {
            color: #07a;
        }

        .richText    .token.function {
            color: #DD4A68;
        }

        .richText     .token.regex,
        .richText     .token.important,
        .richText     .token.variable {
            color: #e90;
        }

        .richText    .token.important,
        .richText    .token.bold {
            font-weight: bold;
        }
        .richText      .token.italic {
            font-style: italic;
        }

        .richText     .token.entity {
            cursor: help;
        }

        /* Custom prism rules for the Relay docs */

        .richText    code[class*="language-"],
        .richText    pre[class*="language-"] {
            margin-bottom: 10px;
        }
        .richText   code[class*="language-"],
        .richText    pre[class*="language-"],
        .richText    .apiIndex pre {
            font-family: 'source-code-pro', Menlo, 'Courier New', Consolas, monospace;
            white-space: pre-wrap;
        }

        .richText   code[class*="language-"],
        .richText   pre[class*="language-"],
        .richText   .apiIndex li {
            background-color: rgba(75, 145, 80, 0.05);
            border-left: 4px solid #C15757;
            font-size: 13px;
            line-height: 20px;
            padding: 5px 10px;
        }

        .richText   .prism + .prism {
            margin-top: -15px;
        }

        /* Line highlighting */

        .richText   .prism {
            position: relative;
        }

        .richText  .line-highlight {
            background: rgba(75, 145, 80, 0.15);
            left: 0;
            line-height: inherit;
            margin-top: 5px;
            pointer-events: none;
            position: absolute;
            right: 0;
        }

    </style>
</head>
